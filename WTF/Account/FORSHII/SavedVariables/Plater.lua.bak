["Nameplate Updated"] = "function (self, unitId, unitFrame, envTable, modTable)\n    \n    \n    local healthPercent = 0\n    \n    if envTable.unitIsPlayer or envTable.unitIsNonTrivial then\n        healthPercent = unitFrame.healthBar.CurrentHealth / unitFrame.healthBar.CurrentHealthMax\n    end\n    \n    \n    if unitFrame.namePlateUnitReaction <= 4 and unitFrame.healthBar.CurrentHealth < UnitHealthMax (\"player\") and unitFrame.InCombat and  healthPercent < 15 and GetSpellCooldown(322109) == 0 then\n        Plater.StartGlow(unitFrame.healthBar, modTable.AntsColor, modTable.options)\n    else\n        Plater.StopGlow(unitFrame.healthBar, modTable.options.glowType, modTable.options.key)\n    end\n    \n    \nend\n\n\n\n\n",
["Initialization"] = "function (modTable)\n    \n    -- for the LibCustomGlow implementation (default):\n    modTable.options = {\n        glowType = \"pixel\",\n        color = \"#ff4200\", -- all plater color types accepted, from lib: {r,g,b,a}, color of lines and opacity, from 0 to 1. Defaul value is {0.95, 0.95, 0.32, 1}\n        N = 12, -- number of lines. Defaul value is 8;\n        frequency = 0.25, -- frequency, set to negative to inverse direction of rotation. Default value is 0.25;\n        length = 6, -- length of lines. Default value depends on region size and number of lines;\n        th = 1, -- thickness of lines. Default value is 2;\n        xOffset = 0,\n        yOffset = 0, -- offset of glow relative to region border;\n        border = false, -- set to true to create border under lines;\n        key = \"TouchOfDeath\", -- key of glow, allows for multiple glows on one frame;\n    }\nend\n\n\n",
["Nameplate Added"] = "function (self, unitId, unitFrame, envTable, modTable)\n    envTable.unitIsPlayer = unitFrame.ActorType == \"enemyplayer\"\n    envTable.unitIsNonTrivial = unitFrame.namePlateClassification and (unitFrame.namePlateClassification == \"elite\" or unitFrame.namePlateClassification == \"rareelite\" or unitFrame.namePlateClassification == \"rare\" or unitFrame.namePlateClassification == \"worldboss\")\nend\n\n\n",
},
